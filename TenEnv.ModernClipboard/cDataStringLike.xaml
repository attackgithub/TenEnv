<UserControl x:Class="TenEnv.ModernClipboard.cDataStringLike"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TenEnv.ModernClipboard"
             mc:Ignorable="d" 
             d:DesignHeight="116" d:DesignWidth="288">
    <Grid>
        <TextBlock x:Name="tb" Margin="13,13,13,35" HorizontalAlignment="Left" VerticalAlignment="Top" TextWrapping="Wrap">Content</TextBlock>
        <TextBlock x:Name="tbFormat" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="16" FontSize="14">HTML</TextBlock>
    </Grid>
</UserControl>
